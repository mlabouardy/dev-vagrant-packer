{
  "variables" : {
     "aws_secret_key" : "",
     "aws_access_key" : ""
  },
  "builders" : [
    {
      "type" : "amazon-ebs",
      "access_key" : "{{ user `aws_access_key` }}",
      "secret_key" : "{{ user `aws_secret_key` }}",
      "region" : "us-east-1",
      "source_ami" : "ami-e600d1f0",
      "instance_type" : "t2.micro",
      "ssh_username" : "ubuntu",
      "ami_name" : "packer-example {{timestamp}}"
    }
 ],
 "provisioners": [
   {
      "type" : "shell",
      "inline" : [
         "sleep 30",
         "sudo apt-get update",
         "sudo apt-get install -y redis-server"
      ]
   }
 ]
}
